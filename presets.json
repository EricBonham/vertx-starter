[
  {
    "id": "webapp + mongo",
    "description": "A simple webapp backed by mongoDB",
    "dependencies": [
      "io.vertx:vertx-core",
      "io.vertx:vertx-web",
      "io.vertx:vertx-web-templ-jade",
      "io.vertx:vertx-mongo-client"
    ],
    "buildtool": "maven",
    "language": "java",
    "templates": [
      "web+mongo/src/main/java/{package}/MainVerticle.java",
      "web+mongo/src/main/resources/templates/index.jade",
      "web+mongo/src/main/resources/templates/layout.jade",
      "web+mongo/src/main/resources/webroot/css/style.css",
      "web+mongo/src/main/resources/webroot/js/app.js"
    ]
  },
  {
    "id": "service proxy application",
    "description": "A bare bones service proxy application",
    "dependencies": [
      "io.vertx:vertx-core",
      "io.vertx:vertx-codegen",
      "io.vertx:vertx-service-proxy"
    ],
    "buildtool": "maven",
    "language": "java",
    "templates": [
      "maven+service-proxy/pom.xml",
      "maven+service-proxy/src/main/java/{package}/Service.java",
      "maven+service-proxy/src/main/java/{package}/MainVerticle.java",
      "maven+service-proxy/src/main/java/{package}/package-info.java",
      "maven+service-proxy/src/main/java/{package}/impl/ServiceImpl.java"
    ]
  }
]
