{
  "dependencies": [
  {{#each dependencies}}
    {
      "groupId": "{{ groupId }}",
      "artifactId": "{{ artifactId }}",
      "version": "{{ version }}",
      {{#if classifier}}
      "classifier": "{{ classifier }}",
      {{/if}}
      {{#if transitive }}
      "transitive": {{ transitive }},
      {{/if}}
      "included": {{ checked }}
    }{{#unless @last}},{{/unless}}
  {{/each}}
  ]
}
